<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Selenium Playground Tests" parallel="classes" thread-count="4">
    <test name="Simple Form Demo Tests">
        <parameter name="browser" value="Chrome"/>
        <parameter name="version" value="128.0"/>
        <parameter name="os" value="Windows 10"/>
        <classes>
            <class name="com.simpleform.SimpleFormTest"/>
        </classes>
    </test>
    
    <test name="Simple Form Demo Tests - Edge">
        <parameter name="browser" value="MicrosoftEdge"/>
        <parameter name="version" value="127.0"/>
        <parameter name="os" value="macOS Ventura"/>
        <classes>
            <class name="com.simpleform.SimpleFormTest"/>
        </classes>
    </test>
    
    <test name="Simple Form Demo Tests - Firefox">
        <parameter name="browser" value="Firefox"/>
        <parameter name="version" value="130.0"/>
        <parameter name="os" value="Windows 11"/>
        <classes>
            <class name="com.simpleform.SimpleFormTest"/>
        </classes>
    </test>
    
    <test name="Simple Form Demo Tests - IE">
        <parameter name="browser" value="Internet Explorer"/>
        <parameter name="version" value="11.0"/>
        <parameter name="os" value="Windows 10"/>
        <classes>
            <class name="com.simpleform.SimpleFormTest"/>
        </classes>
    </test>
</suite>